{
  "expo": {
    "name": "Auth App",
    "slug": "react-native-tailwind-redux-toolkit-setup",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/app.png",
    "scheme": "reactnativetailwindreduxtoolkitsetup",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/app.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.roushan0209.reactnativetailwindreduxtoolkitsetup"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/app.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/app.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "b8912fad-9fe6-44be-a6b1-710d0f5fb238"
      }
    }
  }
}
